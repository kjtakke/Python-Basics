import pandas as pd
from pandas import DataFrame

df1 = pd.DataFrame(
	{
		"id":[4,2,1],
		"FName":["John","Kris","Mat"]
		
	}
	
)
print(df1)
print("")

df2 = pd.DataFrame(
	{
		"id":[1,2,4],
		"LName":["Ryan","Jone","Pip"]
		
	}
	
)
print(df2)
print("")

df = pd.merge(
df1,
df2,
how='inner',
left_on="id",
right_on="id",
indicator=True
)

print(df)
